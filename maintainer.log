<script>
  function toggleTheme() {
    const body = document.body;
    const lightCss = "light-theme";
    const darkCss = "dark-theme";

    if (body.classList.contains(lightCss)) {
      body.classList.remove(lightCss);
      body.classList.add(darkCss);
